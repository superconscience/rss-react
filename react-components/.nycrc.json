{
  "all": true,
  "check-coverage": true,
  "extends": "@istanbuljs/nyc-config-typescript",
  "reporter": ["text-summary"],
  "temp-dir": "./dist/.nyc",
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": [
    "cypress/**/*.*",
    "**/main.tsx",
    "src/server/**/*",
    "src/tests/**/*",
    "**/*.d.ts"
  ]
}
