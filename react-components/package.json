{
  "name": "react-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server/server.ts",
    "start:server": "ts-node src/server/server.ts",
    "test:unit": "vitest run --coverage",
    "test:e2e": "set \"NODE_TEST=e2e\" && start-server-and-test start:server http://localhost:5173 test:cypress && npx nyc report --reporter=text-summary",
    "test:cypress": "cypress run --e2e",
    "preview": "vite preview",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "eslint": "eslint . --ext .tsx --ext .ts",
    "lint": "npm run check-types && npm run eslint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "classnames": "^2.3.2",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "nodemon": "^2.0.22",
    "open": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.1",
    "@types/whatwg-fetch": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "abort-controller": "^3.0.0",
    "concurrently": "^8.0.1",
    "cross-fetch": "^3.1.5",
    "cypress": "^12.10.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.4",
    "sass": "^1.59.2",
    "serve-static": "^1.15.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3",
    "whatwg-fetch": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && git add -A ."
    }
  }
